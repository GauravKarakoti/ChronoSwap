{
    "targets": {
        "release": {
            "sourceMap": true,
            "optimizeLevel": 3,
            "shrinkLevel": 3,
            "converge": true,
            "noAssert": false,
            "exportRuntime": true,
            "bindings": false,
            "outFile": "build/main.wasm"
        }
    },
    "options": {
        "exportTable": true,
        "importTable": true,
        "use": ["ASC_RTRACE=1"],
        "paths": {
          "as-json": [
            "./node_modules/as-json/assembly/index.ts"
          ]
        }
    },
    "entries": [
        "./assembly/contracts/main.ts"
    ]
}