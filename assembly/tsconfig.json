{
  "extends": "assemblyscript/std/assembly.json",
  "include": [
    "**/*.ts"
  ],
  "compilerOptions": {
    "paths": {
      "as-json": [
        "../node_modules/as-json/assembly/index.ts"
      ]
    }
  },
  "typedocOptions": {
    "exclude": "assembly/**/__tests__",
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "includeVersion": true,
    "skipErrorChecking": true
  }
}